
public class Demo {

	public static void main(String[] args) {
		Visitor visitor1 = new Visitor("Vandy", 150.0, 20.0);
		System.out.println(visitor1.toString());
		
		SilverMembership silver = new SilverMembership("Vandy", 150.0, 20.0);
		System.out.println(visitor1.toString());
		
		GoldMembership gold = new GoldMembership("Vandy", 150.0, 20.0);
		System.out.println(visitor1.toString());
	}

}
