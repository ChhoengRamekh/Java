package ClassInheritance;

public class Animal {
	public String name;
	public String breed;
	public String color;
	public String age;
	
	public Animal() {
		
	}
	
	public void eat() {
		System.out.println("I could eat everything");
	}
	
	public void run() {
		System.out.println("I could run fast than people");
	}
	
}
