
public class Student {
	String name;
	String province;
	private int age;
	private String batch;
	
	public Student() {
		
	}
	public Student( String name, String province, int age, String batch){
		
	}
	public Student(String name, String province, int age) {
		
	}
	
	
	// Setter and getter for age
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
	// Setter and getter for batch
	public String getBatch() {
		return batch;
	}
	public void setBatch(String batch) {
		this.batch = batch;
	}
	
	
	
}
